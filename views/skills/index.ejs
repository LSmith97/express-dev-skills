<%- include('../partials/header') %>

    <h1><%= title%></h1>
    <% skills.forEach(function(skill){%>
        <h3>
            <a href="skills/<%=skill.skill%>"><%= skill.skill %></a>
        </h3>
        <a href="skills/<%=skill.skill%>/new">Add skill</a>
        <ul>
            <% skill.proficiencies.forEach(function(proficiency){%>
                <li><%= proficiency %></li>
            <%}); %>
        </ul>
        
    <%});%>

<%- include('../partials/footer') %>